{
  "kind": "collectionType",
  "collectionName": "team_members",
  "info": {
    "singularName": "team-member",
    "pluralName": "team-members",
    "displayName": "Team Member",
    "description": "Legal team members and their information"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "position": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "description": {
      "type": "text",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "phone": {
      "type": "string",
      "maxLength": 50
    },
    "whatsapp": {
      "type": "string",
      "maxLength": 50
    },
    "email": {
      "type": "email"
    },
    "experience": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
