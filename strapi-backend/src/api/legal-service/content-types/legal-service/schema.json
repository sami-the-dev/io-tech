{
  "kind": "collectionType",
  "collectionName": "legal_services",
  "info": {
    "singularName": "legal-service",
    "pluralName": "legal-services",
    "displayName": "Legal Service",
    "description": "Detailed legal services for the About page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 255
    },
    "description": {
      "type": "text",
      "required": true
    },
    "subtitles": {
      "type": "component",
      "repeatable": true,
      "component": "content.subtitle"
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
